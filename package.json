{
	"name": "emojisense",
	"main": "./lib/emojisense",
	"version": "0.3.1",
	"description": "A simple markdown emoji autocompleter inspired by VSCode's :emojisense:",
	"scripts": {
		"build": "babel src/ -d lib/",
		"dev": "babel src/ -s -d lib/ --watch"
	},
	"keywords": [
		"makrdown",
		"emoji",
		"intellisense",
		"autocomplete"
