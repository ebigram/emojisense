	"license": "MIT",
	"engines": {
		"inkdrop": "^5.x"
	},
	"devDependencies": {
		"babel-plugin-ramda": "^2.0.0",
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-react": "^7.9.4",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"prettier": "^2.0.4"
